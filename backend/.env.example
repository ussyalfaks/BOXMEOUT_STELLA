# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/boxmeout_dev?schema=public"
DATABASE_URL_TEST="postgresql://postgres:password@localhost:5432/boxmeout_test?schema=public"

# Server
PORT=3000
NODE_ENV=development

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=your-super-secret-refresh-token-key
REFRESH_TOKEN_EXPIRES_IN=30d

# Wallet Auth JWT (for Stellar wallet authentication)
JWT_ACCESS_SECRET=your-super-secret-access-key-min-32-chars-here
JWT_REFRESH_SECRET=your-super-secret-refresh-key-min-32-chars-here
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=7d

# Encryption (for salt storage in commit-reveal predictions)
ENCRYPTION_KEY=your-32-byte-encryption-key-here-change-in-production!!

# Stellar Network
STELLAR_NETWORK=testnet
STELLAR_HORIZON_URL=https://horizon-testnet.stellar.org
STELLAR_SOROBAN_RPC_URL=https://soroban-testnet.stellar.org

# Contract Addresses (set after deployment)
FACTORY_CONTRACT_ADDRESS=
MARKET_CONTRACT_ADDRESS=
TREASURY_CONTRACT_ADDRESS=
ORACLE_CONTRACT_ADDRESS=
AMM_CONTRACT_ADDRESS=
USDC_TOKEN_ADDRESS=

# Admin
ADMIN_WALLET_SECRET=
ADMIN_WALLET_ADDRESSES=

# Redis (optional)
REDIS_URL=redis://localhost:6379

# CORS
CORS_ORIGIN=http://localhost:5173
